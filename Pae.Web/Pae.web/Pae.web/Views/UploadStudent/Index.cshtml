@model IEnumerable<Pae.web.Data.Entities.Estudents>
@{Layout = null;
}

    <form asp-controller="UploadStudent" asp-action="Index" method="post" enctype="multipart/form-data">
        <input type="file"  name="file" value="" />
        <button type="submit"> Upload </button>
    </form>

@if (Model.Count() >0)
{
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <td>Fullname</td>
            <td>Documento</td>
            <td>Intitucion</td>
        </tr>
        @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Document)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Site)
        </td>
    </tr>
        }
    </table>

}